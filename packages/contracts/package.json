{
  "name": "contracts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "build",
    "build": "npx hardhat compile && typechain --target=ethers-v5  \"./build/abis/**/*.json\" --out-dir ./../../node_modules/@contracts-sdk",
    "test": "npx hardhat test",
    "format": "prettier --write \"tests/**/*.ts\" \"tasks/**/*.ts\" \"contracts/**/*.sol\" \"deploy/**/*.ts\" \"hardhat.config.ts\"",
    "pretty-quick": "pretty-quick --staged",
    "deploy": "hardhat --network ethereum deploy",
    "verify": "hardhat --network ethereum etherscan-verify --solc-input",
    "export": "hardhat --network ethereum export --export deployments/ethereum.json"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "1.4.8",
    "@nomiclabs/hardhat-ethers": "2.2.1",
    "@nomiclabs/hardhat-etherscan": "3.1.3",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@openzeppelin/contracts": "4.8.0",
    "@openzeppelin/contracts-upgradeable": "4.8.0",
    "@openzeppelin/hardhat-upgrades": "1.22.0",
    "@typechain/ethers-v5": "10.2.0",
    "@typechain/hardhat": "6.1.5",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.17",
    "chai": "4.3.7",
    "env-var": "^7.3.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "5.7.2",
    "hardhat": "2.12.4",
    "hardhat-abi-exporter": "2.10.1",
    "hardhat-deploy": "0.11.22",
    "hardhat-gas-reporter": "1.0.9",
    "husky": "8.0.2",
    "prettier": "2.8.1",
    "prettier-plugin-solidity": "^1.1.0",
    "pretty-quick": "3.1.3",
    "solidity-coverage": "0.8.2",
    "standard-version": "9.5.0",
    "ts-generator": "0.1.1",
    "ts-node": "10.9.1",
    "typechain": "8.1.1",
    "typescript": "4.9.4"
  }
}
